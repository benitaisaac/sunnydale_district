/* === GENERAL APP STYLE === */
QMainWindow {
    font-family: 'Segoe UI', sans-serif;
    font-size: 14px;
    color: #333;
}

 QWidget {
    background-color: rgb(255, 252, 228);
    font-family: 'Segoe UI', sans-serif;
    font-size: 14px;
    color: #333;
}
/* === PUSH BUTTONS === */
QPushButton {
    background-color: rgb(5, 60, 171);
    color: white;
    border: none;
    border-radius: 6px;
    padding: 6px 12px;
}

QPushButton:hover {
    background-color: rgb(187, 228, 255);
}

QPushButton:pressed {
    background-color: #2e5aa0;
}

/* === COMBOBOX, INPUTS, TEXTAREA === */
QComboBox, QLineEdit, QTextEdit, QDateEdit, QTimeEdit {
    background-color: #ffffff;
    border: 1px solid #ccc;
    border-radius: 4px;
    padding: 4px 6px;
}

QComboBox::drop-down {
    border: none;
    background-color: #ffffff;
}

QComboBox QAbstractItemView {
    background-color: #ffffff;
    border: 1px solid #ccc;
    selection-background-color: #e6f0ff;
    selection-color: #000;
}


/* === TABLES === */
QTableWidget, QTableView {
    background-color: #ffffff;
    alternate-background-color: #f0f0f0;
    gridline-color: #dcdcdc;
    border: 1px solid #ccc;
    border-radius: 4px;
}

QHeaderView::section {
    background-color: #e6e9ed;
    padding: 4px;
    border: none;
    font-weight: bold;
}

/* === LIST WIDGETS === */
QListWidget {
    background-color: #ffffff;
    border: 1px solid #ccc;
    border-radius: 4px;
}

/* === CALENDAR === */
QCalendarWidget QToolButton {
    color: #4a90e2;
}

QCalendarWidget QAbstractItemView {
    selection-background-color: rgb(5, 60, 171);
    selection-color: white;
}

/* === GROUP BOXES === */
QGroupBox {
    border: 1px solid #aaa;
    border-radius: 6px;
    margin-top: 6px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 4px;
    background-color: transparent;
    color: #4a90e2;
    font-weight: bold;
}

/* === TABS === */
QTabWidget::pane {
    border: 1px solid #ccc;
    border-radius: 4px;
	background-color: rgb(187, 228, 255);
}

QTabBar::tab {
    background: #e6e9ed;
    border: 1px solid #ccc;
    border-bottom: none;
    padding: 6px 12px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background: #ffffff;
    color: rgb(5, 60, 171);
    padding: 6px 12px;
   	font-weight: bold;
}

/* === TOOLTIP === */
QToolTip {
    background-color: #333;
    color: #fff;
    padding: 4px;
    border-radius: 4px;
}

QScrollBar:vertical, QScrollBar:horizontal {
    background: white;
    border: 1px solid #ccc;
    border-radius: 6px;
}

QCheckBox {
    background-color: white;
}

QMessageBox {
    background-color: white;
    font-size: 15px;
}

QMessageBox QLabel {
    background-color: transparent;
}


